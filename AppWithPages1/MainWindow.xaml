<Window x:Class="AppWithPages1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppWithPages1"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1200">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="7*"/>
			<ColumnDefinition Width="17*"/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="100*"/>
			<RowDefinition Height="800*"/>
		</Grid.RowDefinitions>
		<Frame x:Name="frameMain" 
		Grid.Column="1"
		MinHeight="450" MinWidth="450"
		Background="#C05000"
		Source="HomePage.xaml" Margin="0,98,0,0" Grid.RowSpan="2"
		/>

		<StackPanel x:Name="pnlPageListings"
		Grid.Column="0"
		
		Background="#905000"
		Orientation="Vertical" Margin="0,98,0,0" Grid.RowSpan="2"  >
			<TextBlock  FontSize="30" >
				Sample Pages
			</TextBlock>

			<Button Click="Button_Click">
				<TextBlock  FontSize="30" >
				Sample Pages
				</TextBlock> 
			</Button>



		</StackPanel>



	</Grid>
</Window>
